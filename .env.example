# Social Media Credentials Template
# Copy to .env and fill in your credentials
# All variables can be prefixed with SOCIALIA_ or SCITEX_

# X/Twitter API (get from developer.x.com)
SOCIALIA_X_CONSUMER_KEY=
SOCIALIA_X_CONSUMER_KEY_SECRET=
SOCIALIA_X_ACCESSTOKEN=
SOCIALIA_X_ACCESSTOKEN_SECRET=

# LinkedIn (OAuth 2.0 access token)
SOCIALIA_LINKEDIN_ACCESS_TOKEN=

# Reddit API
SOCIALIA_REDDIT_CLIENT_ID=
SOCIALIA_REDDIT_CLIENT_SECRET=
SOCIALIA_REDDIT_USERNAME=
SOCIALIA_REDDIT_PASSWORD=

# Slack Bot (get from api.slack.com/apps)
SOCIALIA_SLACK_BOT_TOKEN=xoxb-your-bot-token
SOCIALIA_SLACK_DEFAULT_CHANNEL=C08TMRA20AD

# YouTube API
SOCIALIA_YOUTUBE_CLIENT_SECRETS_FILE=

# Google Analytics
SOCIALIA_GOOGLE_ANALYTICS_MEASUREMENT_ID=
SOCIALIA_GOOGLE_ANALYTICS_API_SECRET=
SOCIALIA_GOOGLE_ANALYTICS_PROPERTY_ID=
